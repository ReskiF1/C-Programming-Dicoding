#include <stdio.h>

int main() {
    
    int TRUE = 1;
    int FALSE = 0;
    
    printf("OR  T   F\n");
    printf("T   ");
    printf("%c   ", (TRUE || TRUE) ? 'T' : 'F');
    printf("%c\n", (TRUE || FALSE) ? 'T' : 'F');
    printf("F   ");
    printf("%C   ", (FALSE || TRUE) ? 'T': 'F');
    printf("%c\n", (FALSE || FALSE) ? 'T': 'F');

    return 0;
}
